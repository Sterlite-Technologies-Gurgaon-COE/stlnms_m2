            com.fasterxml.jackson.databind.type;version="[2,4)";resolution:=optional,
            io.netty.channel;resolution:=optional,
            io.netty.channel.nio;resolution:=optional,
            io.netty.util.concurrent;resolution:=optional,
            javax.jms,
